{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/ultra",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
